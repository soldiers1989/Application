<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:padding="@dimen/chad_10dp">

    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/text_province"
        style="@style/TextStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/layout_dieselOil0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/chad_5dp"
        android:paddingLeft="@dimen/chad_50dp"
        app:layout_constraintTop_toBottomOf="@id/text_province">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_title_dieselOil0"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/dieselOil0"
            android:textSize="@dimen/chad_16sp" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_dieselOil0"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/chad_10dp"
            android:textSize="@dimen/chad_16sp"
            app:layout_constraintLeft_toRightOf="@id/text_title_dieselOil0" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/layout_gasoline90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/chad_5dp"
        android:paddingLeft="@dimen/chad_50dp"
        app:layout_constraintTop_toBottomOf="@id/layout_dieselOil0">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_title_gasoline90"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gasoline90"
            android:textSize="@dimen/chad_16sp" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_gasoline90"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/chad_10dp"
            android:textSize="@dimen/chad_16sp"
            app:layout_constraintLeft_toRightOf="@id/text_title_gasoline90" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/layout_gasoline93"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/chad_5dp"
        android:paddingLeft="@dimen/chad_50dp"
        app:layout_constraintTop_toBottomOf="@id/layout_gasoline90">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_title_gasoline93"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gasoline93"
            android:textSize="@dimen/chad_16sp" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_gasoline93"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/chad_10dp"
            android:textSize="@dimen/chad_16sp"
            app:layout_constraintLeft_toRightOf="@id/text_title_gasoline93" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/layout_gasoline97"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/chad_5dp"
        android:paddingLeft="@dimen/chad_50dp"
        app:layout_constraintTop_toBottomOf="@id/layout_gasoline93">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_title_gasoline97"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/gasoline97"
            android:textSize="@dimen/chad_16sp" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/text_gasoline97"
            style="@style/TextStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/chad_10dp"
            android:textSize="@dimen/chad_16sp"
            app:layout_constraintLeft_toRightOf="@id/text_title_gasoline97" />
    </android.support.constraint.ConstraintLayout>


</android.support.constraint.ConstraintLayout>
